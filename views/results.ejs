<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">

    <!-- set viewport to device width to allow responsiveness -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS from CDN -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">

    <!-- link to custom stylesheet (always require after Bootstrap) -->
    <link rel="stylesheet" href="../stylesheets/main.css" media="screen" title="no title" charset="utf-8">
    <!-- jQuery from CDN -->
    <script
      src="https://code.jquery.com/jquery-3.2.1.js"
      integrity="sha256-DZAnKJ/6XZ9si04Hgrsxu/8s717jcIzLy3oi35EouyE="
      crossorigin="anonymous">
    </script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    <script src="../scripts/app.js"></script>
    <!-- link to custom javascript file (always require after external JS libraries) -->
    <!--<script type="text/javascript" src="js/app.js"></script>-->

    <title>eatBetter</title>
</head>

<body>
  <!--main container-->
  <div class="container-fluid">
<!--**********************HEADER ROW***********************-->
    <div class="row headerRow">
      <div class="col-md-3 hamburgerCol">
        <svg class="hamburger" width="37px" height="33px" viewBox="0 0 37 33" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
            <!-- Generator: Sketch 44.1 (41455) - http://www.bohemiancoding.com/sketch -->
            <desc>Created with Sketch.</desc>
            <defs>
                <rect id="path-1" x="0" y="0" width="37" height="9" rx="4.5"></rect>
                <rect id="path-2" x="0" y="12" width="37" height="9" rx="4.5"></rect>
                <rect id="path-3" x="0" y="24" width="37" height="9" rx="4.5"></rect>
            </defs>
            <g id="Landing-Page" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                <g id="Desktop" transform="translate(-23.000000, -116.000000)">
                    <g id="Advanced-Menu" transform="translate(23.000000, 116.000000)">
                        <g id="Rectangle-3">
                            <use fill="#D8D8D8" fill-rule="evenodd" xlink:href="#path-1"></use>
                            <rect stroke="#000000" stroke-width="1" x="0.5" y="0.5" width="36" height="8" rx="4"></rect>
                        </g>
                        <g id="Rectangle-3-Copy">
                            <use fill="#D8D8D8" fill-rule="evenodd" xlink:href="#path-2"></use>
                            <rect stroke="#000000" stroke-width="1" x="0.5" y="12.5" width="36" height="8" rx="4"></rect>
                        </g>
                        <g id="Rectangle-3-Copy-2">
                            <use fill="#D8D8D8" fill-rule="evenodd" xlink:href="#path-3"></use>
                            <rect stroke="#000000" stroke-width="1" x="0.5" y="24.5" width="36" height="8" rx="4"></rect>
                        </g>
                    </g>
                </g>
            </g>
        </svg>
      </div>
      <div class="col-md-6 logoTaglineCol">
        <center><img src='../images/v1Logo.png' class="logo" width="261" height="98"></center>
        <center><p class="tagline">“Write, review and share the menu.”</p></center>
      </div>
      <div class="col-md-3 loginSignupCol">
        <center class="loginSignup">
        <% if(user){ %>
          Hello <%= user.fb.firstName %>
          <a class="btn btn-default logout" href="/logout" role="button">Logout</a>
        <% } else {  %>
          <a class="btn btn-default signup" href="/login/facebook" role="button">Login with Facebook</a>
        <% } %>
        <!-- <input type="button"  class="signup"  value=" Signup "  onclick="document.form.action='http://example.com/registrationPage.php'; document.form.submit();"></center> -->
      </div>
    <!--closes headerRow-->
    </div>
<!--*********************SEARCHBAR ROW*********************-->
<form action="/search" method="GET">
    <div class="row searchbarRow">
      <div class="col-md-6 col-md-offset-3 searchbarCol">
        <input type=search id="searchbar" class="searchbar" results=5 placeholder="Search by: menu item..." autosave=5 name="q">
    <!--closes searchbarRow-->
    </div>
<!--******************TOGGLE BUTTONS ROW*******************-->
    <div class="row toggleButtonsRow">
      <div class="col-md-4 col-md-offset-4 toggleButtonsCol">
        <center class="toggleButtons">
        <input type="submit"  class="restaurant"  value="menu item" >
        <input type="submit"  class="restaurant"  value="restaurant" >
      </center>
      </div>
    <!--closes toggleButtonsRow-->
    </div>
  </form>
<!--********************MOST RECENT ROW********************-->
    <div class="row mostRecentRow">
      <div class="col-md-4 col-md-offset-4 mostRecentCol">
          <% for (var i = 0; i < 10; i++) { %>
            <div class="thumbnail" id=<%= results[i].id%> >
              <div class="card mb-3">
                <img class="card-img-top" src= <%= results[i].image_url %> style="width:60%; height:60%;"  alt="Card image cap">
                <div class="card-block">
                  <h4 class="card-title"> <%= results[i].name %> </h4>
                  <p class="card-text">YELP's Rating: <%= results[i].rating %> </p>
                  <p class="card-text"><small class="text-muted"><a href= <%= results[i].url %> ></a></small></p>
                </div>
              </div>
            </div>
            <% } %>
      </div>
    <!--closes mostRecentRow-->
    </div>
<!--**********************FOOTER ROW***********************-->
    <div class="row footerRow">
      <div class="col-md-12 footerCol">
        <center><p class="copyright">Copyright Now-ish</p></center>
      </div>
    <!--closes footerRow-->
    </div>
<!--*******************************************************-->
  <!--closes main container-->
  </div>
</body>

</html>
